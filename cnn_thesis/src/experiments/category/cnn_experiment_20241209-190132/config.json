  "model": {
    "base": "VGG16",
    "input_shape": [
      224,
      224,
      3
    ],
    "dense_layers": [
      1024
    ],
    "dropout_rate": 0.41,
    "l2_regularization": 8e-05,
    "activation_function": "relu",
    "kernel_initializer": "glorot_normal",
    "use_batch_norm": true,
    "batch_norm_momentum": 0.97
  },
  "training": {
    "learning_rate_decay": {
      "enabled": true,
      "factor": 0.5,
      "patience": 10,
      "min_lr": 1e-06,
      "monitor": "val_loss"
    },
    "optimizer": "adam",
    "learning_rate": 0.0005,
    "optimizer_momentum": 0.5,
    "batch_size": 72,
    "epochs": 100,
    "loss_function": "categorical_crossentropy",
    "deterministic": true,
    "early_stopping": {
      "enabled": true,
      "patience": 7,
      "min_delta": 0.0009,
      "monitor": "val_loss",
      "restore_best_weights": true
    }
  },
  "data_augmentation": {
    "enabled": false,
    "rotation_range": 20,
